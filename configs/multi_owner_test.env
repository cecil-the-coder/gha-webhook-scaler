# Test configuration for multi-owner mode
MULTI_OWNER_MODE=true
OWNERS=testowner1,testorg2

# Configuration for testowner1 (single repository)
OWNER_TESTOWNER1_GITHUB_TOKEN=ghp_test_token_owner1
OWNER_TESTOWNER1_REPO_NAME=my-repo
OWNER_TESTOWNER1_WEBHOOK_SECRET=secret1
OWNER_TESTOWNER1_MAX_RUNNERS=3
OWNER_TESTOWNER1_LABELS=owner1-label,custom

# Configuration for testorg2 (all repositories)
OWNER_TESTORG2_GITHUB_TOKEN=ghp_test_token_org2
OWNER_TESTORG2_ALL_REPOS=true
OWNER_TESTORG2_WEBHOOK_SECRET=secret2
OWNER_TESTORG2_MAX_RUNNERS=10

# Global defaults
MAX_RUNNERS=15
RUNNER_TIMEOUT=3600
PORT=8080
DEBUG=true
RUNNER_IMAGE=myoung34/github-runner:ubuntu-noble
CACHE_VOLUMES=true

# Job scanning configuration (0 to disable, value in seconds)
JOB_SCAN_INTERVAL_SECONDS=300